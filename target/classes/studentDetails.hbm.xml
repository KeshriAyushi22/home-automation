<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping>
	<class name="co.ayu.to.StudentDetails" table="STUDENT_DETAIL">
		<id name="emailID" type="java.lang.String" >
            <column name="emailID" length="255"/>
            <generator class="assigned" />
        </id>
		<property name="studentName" type="java.lang.String">
			<column name="StudentName" />
		</property>
		<property name="studentPhone" type="java.lang.String">
			<column name="mob" />
		</property>
		<property name="collegeNmae" type="java.lang.String">
			<column name="college" />
		</property>
		<property name="collegeID" type="java.lang.String">
			<column name="collegeID" />
		</property>
		<property name="dob" type="java.lang.String">
			<column name="DOB" />
		</property>
		<property name="androidId" type="java.lang.String">
			<column name="androidId" />
		</property>
		<property name="registerToken" type="java.lang.String">
			<column name="registerToken" />
		</property>
		<property name="gender" type="java.lang.String">
			<column name="gender" />
		</property>
		<property name="password" type="java.lang.String">
			<column name="pass" />
		</property>
		<property name="type" type="java.lang.String">
			<column name="type" />
		</property>
		<property name="photo" type="java.lang.String">
			<column name="photo" />
		</property>
			<property name="status" type="java.lang.String">
			<column name="status" />
		</property>
	</class>
</hibernate-mapping>